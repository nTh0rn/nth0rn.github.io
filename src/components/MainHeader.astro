<style>
    .no-scrollbar::-webkit-scrollbar {
    display: none;
    }
    /* Hide scrollbar for IE, Edge and Firefox */
    .no-scrollbar {
    -ms-overflow-style: none;  /* IE and Edge */
    scrollbar-width: none;  /* Firefox */
    }
    .content {
    max-height: 0;
    overflow: hidden;
    transition: max-height 0.4s cubic-bezier(.17,.67,.65,1);
    }
	@keyframes ping-shadow {
  0% {
    box-shadow: 0 0 0 0 rgba(59, 130, 246, 0.7);
    opacity: 1;
  }
  100% {
    box-shadow: 0 0 0 10px rgba(59, 130, 246, 0);
    opacity: 0;
    visibility: hidden; /* hide it completely after */
  }
}

.ping-shadow-animation {
  animation: ping-shadow 1s cubic-bezier(0, 0, 0.2, 1) 5 forwards;
  border-radius: 0.5rem; /* match your rounded-lg */
}
</style>

